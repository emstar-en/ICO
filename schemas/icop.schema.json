{
  "$id": "https://example.org/ico/icop.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "properties": {
    "conditioning_guard": {
      "type": "object"
    },
    "deterministic": {
      "type": "boolean"
    },
    "max_iters": {
      "minimum": 1,
      "type": "integer"
    },
    "retraction_strategy": {
      "enum": [
        "native",
        "approx"
      ],
      "type": "string"
    },
    "tol": {
      "exclusiveMinimum": 0,
      "type": "number"
    }
  },
  "required": [
    "deterministic"
  ],
  "type": "object"
}